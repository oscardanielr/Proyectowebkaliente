<link rel="stylesheet" type="text/css" href="js/rs-plugin/css/settings.css" media="screen">
<link rel="stylesheet" type="text/css" href="/Plantilla/css/isotope.css" media="screen">
<link rel="stylesheet" href="/Plantilla/css/flexslider.css" type="text/css">
<link rel="stylesheet" href="/Plantilla/js/fancybox/jquery.fancybox.css" type="text/css" media="screen">
<link rel="stylesheet" href="/Plantilla/css/bootstrap.css">
<link rel="stylesheet"
  href="https://fonts.googleapis.com/css?family=Noto+Serif:400,400italic,700|Open+Sans:300,400,600,700">

<link rel="stylesheet" href="/Plantilla/css/style.css">
<!-- skin -->
<link rel="stylesheet" href="/Plantilla/skin/default.css">

</head>
{{>navigation_dos}}
<main>

  <!-- section works -->
  <br>
  <br>
  <br>
  <section id="section-works" class="section appear clearfix">
    <div class="container">
      <div class="row">
        <script data-ad-client="ca-pub-9549469569955328" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
      </div>
      <div class="row mar-bot40">
        <div class="col-md-offset-3 col-md-6">
          <div class="section-header">
            <div class="card text-center">
              <div class="card-header">
                <ul class="nav nav-tabs card-header-tabs">
                  <li class="nav-item">
                    <a class="nav-link active" id="color-direccion">DIRECCION</a>
                  </li>
                </ul>
              </div>
              <div class="card-block" id="color-direccion">
                <div class="card" style="width: auto;">
                  <div class="card-block">
                      {{#each contactos}}
                        
                        <h2>{{direccion}}</h2>
                        <hr>
                        <h4>{{telefono_1}}</h4>
                        <hr>
                        <h4>{{telefono_2}}</h4>
                      {{/each}}
                    <p class="card-text"> No lo pienses contactanos y vive la mejor experiencia musical</p>
                    
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="container">
        <div class="row">    
          <div class="col-md-8 ">
            <section id="section-map" class="clearfix">
              <iframe
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d22864.11283411948!2d-73.96468908098944!3d40.630720240038435!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c24fa5d33f083b%3A0xc80b8f06e177fe62!2sNew+York%2C+NY%2C+USA!5e0!3m2!1sen!2sbg!4v1540447494452"
                width="100%" height="380" frameborder="2" style="border:3" allowfullscreen></iframe>
            </section>
          </div>
          <div class="col-md-4 ">
           
              <div class="row" id="colorcont">
                <center>
                  <h2 id="colorcont">CONTACTANOS</h2>
                </center>
                <form action="/route/mensajes" method="POST">
                  <div class="form-group">
                    <input type="text" name="nombre" placeholder="Nombre" class="form-control">
                  </div>
                  <div class="form-group">
                    <input type="email" name="email" placeholder="Correo electronico" class="form-control">
                  </div>
                  <div class="form-group">
                    <input type="text" name="asunto" placeholder="Asunto del mensaje" class="form-control">
                  </div>
                  <div class="form-group">
                   <center>
                      <textarea name="mensaje" id="mensaje" cols="43" rows="10" placeholder="Digite su mensaje"></textarea>
                   </center>
                  </div>
                  <div class="form-group">
                    <button class="btn btn-success btn-block">
                      ENVIAR
                    </button>
                  </div>
                </form>
              </div>
            
          </div>
        </div>
      </div>
    </div>
  </section>


</main>
{{>footer}}